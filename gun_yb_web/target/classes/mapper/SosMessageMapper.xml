  <!--=Start selectSosMessageAll 查询警告信息列表:枪号 -->
  <select id="selectSosMessageAll" resultType="com.ybkj.gun.model.SosMessage">
    SELECT
    g.gun_id,
    g.bluetooth_mac,
    a.app_phone_type_name,
    sm.longitude,
    sm.latitude,
    sm.create_time,
    sm.state
    FROM
    sos_message AS sm
    LEFT JOIN app AS a
    ON sm.app_id = a.id
    LEFT JOIN gun AS g
    ON sm.gun_mac = g.bluetooth_mac
    <where>
      <if test="appCode != null and appCode !=''">
        a.app_phone_type_name like '%' #{appCode} '%'
      </if>
      <if test="gunCode != null and gunCode !=''">
        g.gun_id like '%' #{gunCode} '%'
      </if>
    </where>
    ORDER BY sm.create_time
  </select>
